# Evident yet unstandarized names

# Once you find_package(ROBOTICSLAB_KINEMATICS_DYNAMICS), it defines:
#
# ROBOTICSLAB_KINEMATICS_DYNAMICS_MODULE_DIR    # as in: list(APPEND CMAKE_MODULE_PATH ${ROBOTICSLAB_KINEMATICS_DYNAMICS_MODULE_DIR}) <- not that we use them too much, but you never know...
# ROBOTICSLAB_KINEMATICS_DYNAMICS_DEFINES       # as in: add_definitions(${ROBOTICSLAB_KINEMATICS_DYNAMICS_DEFINES}) <- not that we use them too much, but you never know...
# ROBOTICSLAB_KINEMATICS_DYNAMICS_INCLUDE_DIRS  # as in: include_directories(${ROBOTICSLAB_KINEMATICS_DYNAMICS_INCLUDE_DIRS})  <- IMPORTANT to find headers!
# ROBOTICSLAB_KINEMATICS_DYNAMICS_LINK_DIRS     # as in: link_directories(${ROBOTICSLAB_KINEMATICS_DYNAMICS_LINK_DIRS}) <- not required so much any more, but may be useful.
# ROBOTICSLAB_KINEMATICS_DYNAMICS_LIBRARIES     # as in: target_link_libraries(${KEYWORD} ${ROBOTICSLAB_KINEMATICS_DYNAMICS_LIBRARIES})  <- IMPORTANT to find libraries!
#
# ROBOTICSLAB_KINEMATICS_DYNAMICS_FOUND         # used internally by find_package(TEO)

IF (NOT ROBOTICSLAB_KINEMATICS_DYNAMICS_FOUND)

  SET(ROBOTICSLAB_KINEMATICS_DYNAMICS_MODULE_DIR "@ROBOTICSLAB_KINEMATICS_DYNAMICS_MODULE_DIR@")
  SET(ROBOTICSLAB_KINEMATICS_DYNAMICS_DEFINES "@ROBOTICSLAB_KINEMATICS_DYNAMICS_DEFINES@")
  SET(ROBOTICSLAB_KINEMATICS_DYNAMICS_INCLUDE_DIRS "@ROBOTICSLAB_KINEMATICS_DYNAMICS_INCLUDE_DIRS@")
  SET(ROBOTICSLAB_KINEMATICS_DYNAMICS_LINK_DIRS "@ROBOTICSLAB_KINEMATICS_DYNAMICS_LINK_DIRS@")
  SET(ROBOTICSLAB_KINEMATICS_DYNAMICS_LIBRARIES "@ROBOTICSLAB_KINEMATICS_DYNAMICS_LIBRARIES@")

  SET (ROBOTICSLAB_KINEMATICS_DYNAMICS_FOUND TRUE)
ENDIF (NOT ROBOTICSLAB_KINEMATICS_DYNAMICS_FOUND)
